vector < int > v;

  for (int i = 0; i < 10; i++) {
    v.push_back(i); //inserting elements in the vector
  }
  
 //// how to print the elements
 for(int a : v){
      cout << a << " ";
 }
 vector <pair <int,int >> v;
   v.emplace_back(1,2);
   v.push_back({1,2});       /////note the 2nd brackets in case of push_back
   
   
   
