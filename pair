pair <int,char> p = {1,'S'};
cout << p.first << " " << p.second;

pair <int, pair <int,int>> p = {1, {2,3}};
cout << p.first << " " << p.second.first;

pair <int,int> arr[] = {{1,2},{3,4},{5,6}};
cout << arr[0].second;

pair <int, pair <int,int>> arr[] = {{1,{2,3}},{4,{5,6}},{7,{8,9}}};
cout << arr[1].second.first;
